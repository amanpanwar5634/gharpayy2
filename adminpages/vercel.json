{
  "version": 2,
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/libs/(.*)", "dest": "/libs/$1" },
    { "src": "/fonts/(.*)", "dest": "/fonts/$1" },
    { "src": "/(.*)", "dest": "/login.html" }
  ]
}
